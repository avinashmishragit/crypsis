<h1>Listing rows</h1>

<table>
  <tr>
    <th>Value1</th>
    <th>Value2</th>
    <th>Value3</th>
    <th>Value4</th>
    <th>Column</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @rows.each do |row| %>
  <tr>
    <td><%= row.value1 %></td>
    <td><%= row.value2 %></td>
    <td><%= row.value3 %></td>
    <td><%= row.value4 %></td>
    <td><%= row.column_id %></td>
    <td><%= link_to 'Show', row %></td>
    <td><%= link_to 'Edit', edit_row_path(row) %></td>
    <td><%= link_to 'Destroy', row, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Row', new_row_path %>
